{
    "editor.codeActionsOnSave": {
        "source.fixAll.markdownlint": true,
        "source.fixAll.tslint": true
    },
    "editor.formatOnSave": true,
    "editor.rulers": [
        120
    ],
    "files.associations": {
        ".yarnclean": "ini"
    },
    "spellright.documentTypes": [
        "markdown",
        "plaintext",
        "powershell",
        "typescript"
    ],
    "spellright.ignoreFiles": [
        "**/node_modules"
    ],
    "spellright.language": [
        "en"
    ]
}
